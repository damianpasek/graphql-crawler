{
  "name": "graphql-crawler",
  "version": "1.0.0",
  "description": "GraphQL API with website urls crawler",
  "engines": {
    "node": "12"
  },
  "license": "MIT",
  "scripts": {
    "build": "tsc --outDir dist",
    "dev": "tsc && concurrently -r \"nodemon dist/index.js\" \"tsc -w --project tsconfig.json\"",
    "lint": "eslint src --ext ts,js",
    "typeorm:cli": "ts-node ./node_modules/typeorm/cli.js",
    "test": "TZ=UTC jest .",
    "start": "node dist/index"
  },
  "repository": "https://github.com/damianpasek/graphql-crawler.git",
  "author": "Damian Pasek",
  "dependencies": {
    "apollo-server-express": "^2.13.0",
    "class-validator": "^0.12.2",
    "express": "^4.17.1",
    "graphql": "^15.0.0",
    "graphql-subscriptions": "^1.1.0",
    "mysql": "^2.18.1",
    "puppeteer": "^3.0.4",
    "reflect-metadata": "^0.1.13",
    "ts-node": "^8.10.1",
    "type-graphql": "^1.0.0-rc.1",
    "typeorm": "^0.2.24",
    "typescript": "^3.8.3"
  },
  "devDependencies": {
    "@types/express": "^4.17.6",
    "@types/graphql": "^14.5.0",
    "@types/jest": "^25.2.1",
    "@types/node": "12.7.0",
    "@types/puppeteer": "^2.0.1",
    "@typescript-eslint/eslint-plugin": "^2.31.0",
    "@typescript-eslint/parser": "^2.31.0",
    "concurrently": "^5.2.0",
    "eslint": "^6.8.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "jest": "^26.0.1",
    "nodemon": "^2.0.3",
    "sqlite3": "^4.2.0",
    "ts-jest": "^25.5.1"
  }
}
